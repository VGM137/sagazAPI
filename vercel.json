{
  "name": "sagazAPI",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "github": {
    "enabled": true
  },
  "headers": [
    { "key": "Access-Control-Allow-Credentials", "http://localhost:8080/guisado": "true" }
  ],
  "env": {
    "DB_USER": "@sagaz-db-user",
    "DB_PASSWORD": "@sagaz-db-password",
    "DB_HOST": "@sagaz-db-host",
    "DB_NAME": "@sagaz-db-name"
  }
}